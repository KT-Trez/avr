{
  "build": {
    "appId": "pl.org.was.avr",
    "artifactName": "avr_${version}_${os}_${arch}.${ext}",
    "asar": false,
    "copyright": "Copyright Â© 2022 ${author}",
    "extraMetadata": {
      "main": "build/src_core/main.js"
    },
    "files": [
      "build"
    ],
    "linux": {
      "category": "",
      "icon": "public/icons/logo.png",
      "synopsis": "An app that downloads audio and/or videos from YouTube.",
      "target": [
        "apk",
        "deb",
        "snap"
      ]
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "artifactName": "avr_${version}_${os}_${arch}.${ext}",
      "deleteAppDataOnUninstall": true,
      "license": "LICENSE",
      "menuCategory": "KT-Trez",
      "oneClick": false,
      "perMachine": true
    },
    "productName": "AVR",
    "win": {
      "icon": "public/icons/logo.png",
      "target": [
        "7z",
        "msi",
        "nsis",
        "portable",
        "zip"
      ]
    }
  },
  "config": {
    "default_origin": "http://localhost:3000/"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.0",
    "dotenv": "^16.4.7",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "utils": "^1.0.0",
    "yt-search": "^2.12.1"
  },
  "devDependencies": {
    "@types/yt-search": "^2.10.3",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "types": "^1.0.0"
  },
  "main": "./dist/main.js",
  "name": "electron",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "dev": "electron --trace-warnings .",
    "package": "electron-builder"
  },
  "type": "module",
  "version": "1.1.0"
}
